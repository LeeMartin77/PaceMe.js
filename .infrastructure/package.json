{
  "name": "paceme-infrastructure",
  "version": "0.1.0",
  "bin": {
    ".infrastructure": "bin/.infrastructure.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "cdk:deploy:scaffold": "cdk deploy PaceMeScaffoldStack --outputs-file ./cdk.out/scaffold-outputs.json",
    "cdk:deploy:frontend": "cdk deploy PaceMeFrontendStack --outputs-file ./cdk.out/frontend-outputs.json",
    "cdk:deploy:out": "cdk deploy --all --outputs-file ./cdk.out/outputs.json",
    "script:outfiletoenv": "ts-node ./scripts/write-dev-env-files.ts",
    "script:outfiletocredstash": "./scripts/write-secrets-to-credstash.sh",
    "aws:invalidatecache": "./scripts/invalidate-cache.sh",
    "cdk:deploy:devout": "npm run cdk:deploy:out && npm run script:outfiletoenv"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^10.17.27",
    "@types/nodecredstash": "^2.0.2",
    "aws-cdk": "2.3.0",
    "envfile": "^6.17.0",
    "jest": "^26.4.2",
    "js-yaml": "^4.1.0",
    "nodecredstash": "^2.0.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.3.0",
    "cdk-ecr-deployment": "^2.1.2",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.16"
  }
}
